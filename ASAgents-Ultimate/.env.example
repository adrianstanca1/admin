# =============================================================================
# ASAgents Ultimate - Environment Configuration
# =============================================================================

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================

# MySQL Database Connection (Primary)
DATABASE_URL=mysql://root:your_mysql_password@localhost:3306/asagents_ultimate
MYSQL_ROOT_PASSWORD=your_secure_mysql_password_here
MYSQL_DATABASE=asagents_ultimate

# SQLite Database (Development Fallback)
SQLITE_DATABASE=./database/asagents.sqlite

# =============================================================================
# JWT AUTHENTICATION CONFIGURATION
# =============================================================================

# JWT Secret (IMPORTANT: Generate a secure 256-bit key for production)
JWT_SECRET=your-256-bit-secret-key-here-minimum-32-characters-very-important
JWT_ISSUER=asagents-ultimate
JWT_AUDIENCE=asagents-ultimate-client
JWT_EXPIRES_IN=24h

# =============================================================================
# SERVER CONFIGURATION
# =============================================================================

# Node.js Backend (AI Services)
NODE_ENV=development
PORT=4000

# TypeScript Backend (Core Business)  
SERVER_PORT=4001

# WebSocket Configuration
WS_PORT=4002

# =============================================================================
# FRONTEND CONFIGURATION
# =============================================================================

# API Base URLs
VITE_API_BASE_URL=http://localhost:4000
VITE_JAVA_API_BASE_URL=http://localhost:4001
VITE_WS_URL=ws://localhost:4002

# =============================================================================
# AI INTEGRATION (GOOGLE GEMINI)
# =============================================================================

# Gemini API Configuration
VITE_GEMINI_API_KEY=your_gemini_generative_language_api_key_here
VITE_GEMINI_BROWSER_KEY=your_gemini_browser_api_key_here
GEMINI_GENERATIVE_LANGUAGE_API_KEY=your_gemini_generative_language_api_key_here
GEMINI_MODEL=gemini-1.5-pro

# =============================================================================
# OPENAI INTEGRATION
# =============================================================================

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_MAX_TOKENS=2048

# =============================================================================
# SUPABASE CONFIGURATION
# =============================================================================

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# =============================================================================
# AUTH0 CONFIGURATION
# =============================================================================

# Auth0 Configuration
AUTH0_DOMAIN=your-domain.auth0.com
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_CLIENT_SECRET=your-auth0-client-secret
VITE_AUTH0_DOMAIN=your-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your-auth0-client-id

