# =============================================================================
# FEATURE FLAGS
# =============================================================================

# Backend Configuration
VITE_USE_SUPABASE=false
VITE_USE_AUTH0=true
VITE_ALLOW_MOCK_FALLBACK=true
VITE_ENABLE_REAL_TIME=true

# AI Features
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_GEMINI=true
VITE_ENABLE_OPENAI=true

# Development Features
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_DATA=true

# =============================================================================
# MONITORING & ANALYTICS
# =============================================================================

# Monitoring Configuration
ENABLE_MONITORING=true
HEALTH_CHECK_INTERVAL=30000
ERROR_TRACKING=true

# Analytics
ENABLE_ANALYTICS=true
ANALYTICS_ENDPOINT=https://analytics.asagents.com/api/events

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,https://asagents.vercel.app

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Session Configuration
SESSION_SECRET=your-session-secret-key
SESSION_MAX_AGE=86400000

# =============================================================================
# FILE UPLOAD CONFIGURATION
# =============================================================================

# Upload Settings
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf
UPLOAD_PATH=./uploads